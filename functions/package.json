{
  "name": "functions",
  "version": "1.0.0",
  "__NOTE__:": "avoid hoisting for webpack and other conflicts",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "start": "ts-node scripts/start.ts",
    "serve:live": "yarn copyDevConfig && yarn serve",
    "lint": "tslint --project tsconfig.json",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --watch",
    "copyDevConfig": "firebase functions:config:get > .runtimeconfig.json",
    "shell": "yarn build && firebase functions:shell",
    "test": "firebase emulators:exec --project community-platform-emulated 'jest . --forceExit --detectOpenHandles'",
    "test:watch": "firebase emulators:exec --ui --project community-platform-emulated 'jest . --watch --forceExit --detectOpenHandles'"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/logging": "10.5.0",
    "axios": "1.3.3",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "dateformat": "3.0.3",
    "express": "4.18.2",
    "firebase-admin": "11.4.1",
    "firebase-functions": "4.1.1",
    "firebase-tools": "11.30.0",
    "fs-extra": "11.1.0",
    "google-auth-library": "8.7.0",
    "googleapis": "61.0.0",
    "log-update": "4.0.0",
    "node-fetch": "2.6.9",
    "oa-shared": "workspace:*",
    "request": "2.88.2",
    "sitemap": "7.1.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/concurrently": "6.4.0",
    "@types/cors": "2.8.13",
    "@types/dateformat": "3.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.0",
    "@types/request": "2.48.8",
    "@types/uuid": "9.0.0",
    "@types/webpack": "5.28.0",
    "@types/webpack-node-externals": "2.5.3",
    "awesome-typescript-loader": "5.2.1",
    "concurrently": "6.5.1",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "firebase-functions-test": "3.0.0",
    "jest": "29.2.2",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typescript": "5.1.6",
    "webpack": "5.76.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "engines": {
    "node": "18"
  },
  "private": true
}
